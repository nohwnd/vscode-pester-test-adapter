{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Pester adapter",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
